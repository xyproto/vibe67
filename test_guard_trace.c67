// Trace guard evaluation
test = (x) {
    {
        | (println("evaluating guard 1") == 0 or x == 0) => (println("guard 1 matched") == 0 or 100)
        | (println("evaluating guard 2") == 0 or x == 1) => (println("guard 2 matched") == 0 or 200)
        ~> (println("default matched") == 0 or 999)
    }
}

println(f"test(0) = {test(0)}")
println("")
println(f"test(1) = {test(1)}")
println("")
println(f"test(5) = {test(5)}")
