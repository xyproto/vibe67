fib = (n) {
    | n < 2 => n
    ~> fib(n - 1) + fib(n - 2)
}

println("Computing Fibonacci numbers...")
println(f"fib(0) = {fib(0)}")
println(f"fib(1) = {fib(1)}")
println(f"fib(5) = {fib(5)}")
println(f"fib(10) = {fib(10)}")
println(f"fib(15) = {fib(15)}")
println(f"fib(20) = {fib(20)}")
println(f"fib(25) = {fib(25)}")
println(f"fib(30) = {fib(30)}")
println("Done!")
